@Table(name = "t_member_home")
public class MemberHome extends BaseEntity implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -1440396117469014736L;

	@Column(name = "members")
	private String members;
	@Column(name = "contact")
	private String contact;
	@Column(name = "member_id")
	private Integer memberId;
	@Column(name = "family_relationship")
	private Integer familyRelationship;

	@Column(name = "sex")
	private Integer sex;
	@Column(name = "political")
	private Integer political;
	@Column(name = "id_card")
	private String idCard;
	@Column(name = "health")
	private Integer health;
	@Column(name = "monthly_income")
	private BigDecimal monthlyIncome;
	@Column(name = "identity")
	private String identity;
	@Column(name = "medical_insurance")
	private Integer medicalInsurance;
	@Column(name = "unit_school")
	private String unitSchool;

	public Integer getSex() {
		return sex;
	}

	public void setSex(Integer sex) {
		this.sex = sex;
	}

	public Integer getPolitical() {
		return political;
	}

	public void setPolitical(Integer political) {
		this.political = political;
	}

	public String getIdCard() {
		return idCard;
	}

	public void setIdCard(String idCard) {
		this.idCard = idCard;
	}

