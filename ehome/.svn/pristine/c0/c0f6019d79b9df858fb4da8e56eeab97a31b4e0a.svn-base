<body>
	<div >
	  
	  <div class="clearfix">
							<div class="panel-body">
								<form class="form-inline" role="form" name="searchForm">
								     <input type="hidden" class="form-control" name="groupId" placeholder="groupId" value="${groupId!}">
									<div class="form-group">
										<input type="text" class="form-control" name="userName" placeholder="用户编号、姓名">
									</div>
									
									<button type="submit" name="search" class="btn btn-success">检索</button>
									<button type="submit" name="reset" class="btn btn-success">重置检索</button>
								</form>
							</div>
						</div>
	  <table class="table table-striped table-hover table-bordered" id="add-user-group-list">
			<thead>
				<tr>
					<th width="5%"><input type="checkbox"  class="check-all" id="zcheckbox" /> <span class="lbl"></span>
					<th width="10%" class="hidden-phone">序号</th>
					<th width="10%" class="hidden-phone">后台登录号</th>
					<th width="10%" class="hidden-phone">姓名</th>
					<th width="10%" class="hidden-phone">角色</th>
					<th width="10%" class="hidden-phone">所属工会</th>
					<th width="10%" class="hidden-phone">部门</th>
					<th width="10%" class="hidden-phone">注册时间</th>
					<th width="10%" class="hidden-phone">备注</th>
					
				</tr>
			</thead>
			<tbody>
                  
			</tbody>
	  </table>
	  <div class="modal-footer">
		<button data-dismiss="modal" name="cancelAuth" class="btn btn-default" type="button">取消</button>
		<button class="btn btn-success" name="submitAuth" type="button">加入</button>
	  </div>
	</div>
	<script src="${ctxPath}/static/system/olive/all/js/jquery-1.8.3.min.js"></script>

	<!-- <script src="${ctxPath}/static/ui/dialog/js/jquery.min.js"></script>-->
